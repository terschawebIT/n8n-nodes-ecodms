{
  "name": "n8n-nodes-ecodms",
  "version": "0.1.6",
  "description": "Custom n8n node for ecoDMS document management system",
  "keywords": [
    "n8n-community-node-package",
    "ecoDMS",
    "DMS",
    "Dokumentenmanagement",
    "n8n-nodes-ecodms",
    "document management"
  ],
  "license": "MIT",
  "homepage": "https://www.ecodms.de",
  "author": {
    "name": "Niko Terschawetz",
    "email": "nt@terschaweb.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terschawebIT/n8n-nodes-ecodms.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EcoDmsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EcoDMS/EcoDMS.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.19.86",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "n8n-workflow": "~1.82.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "n8n-workflow": "~1.82.0"
  }
}
