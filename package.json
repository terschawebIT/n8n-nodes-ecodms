{
  "name": "n8n-nodes-ecodms",
  "version": "0.2.3",
  "description": "Custom n8n node for ecoDMS document management system",
  "keywords": [
    "n8n-community-node-package",
    "ecoDMS",
    "DMS",
    "Dokumentenmanagement",
    "n8n-nodes-ecodms",
    "document management"
  ],
  "license": "MIT",
  "homepage": "https://www.ecodms.de",
  "author": {
    "name": "Niko Terschawetz",
    "email": "nt@terschaweb.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terschawebIT/n8n-nodes-ecodms.git"
  },
  "engines": {
    "node": ">=20.15.0",
    "npm": ">=10.0.0"
  },
  "main": "dist/nodes/EcoDMS/EcoDMS.node.js",
  "exports": {
    ".": "./dist/nodes/EcoDMS/EcoDMS.node.js",
    "./credentials": "./dist/credentials/EcoDmsApi.credentials.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "mkdir -p dist/nodes dist/credentials && cp -r nodes/**/*.svg dist/nodes/ && cp -r credentials/*.svg dist/credentials/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts'",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "clean": "rimraf dist/ node_modules/ package-lock.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EcoDmsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EcoDMS/EcoDMS.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.12",
    "@types/memory-cache": "^0.2.6",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.7.0",
    "n8n-workflow": "~1.82.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "memory-cache": "^0.2.0",
    "winston": "^3.11.0"
  },
  "peerDependencies": {
    "n8n-workflow": "~1.82.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/nodes/**/*.test.ts"
    ]
  }
}
